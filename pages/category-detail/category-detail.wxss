.container{
  display: flex;
  flex-direction: column;
}

.head-container{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.headerImage{
  width: 100%;
  height: 400rpx;
}

.category-container{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: absolute;
}

.category-name{
  color: #ffffff;
  font-size: 44rpx;
  font-weight: bold;
}

.category-desc{
  color: #ffffff;
  font-size: 24rpx;
  margin-top: 10rpx;
}

