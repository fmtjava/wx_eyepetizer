.container{
  display: flex;
  flex-direction: column;
}

.banner-swiper{
  margin-top: 100rpx;
  width: 100%;
  height: 400rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
}

.banner-swiper-item{
  width: 100%;
  border-radius: 8rpx;
}

.banner-image{
  width: 100%;
  border-radius: 8rpx;
}

.title-container{
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 20rpx;
}

.title{
  font-size: 36rpx;
  color: #000000DD;
  font-weight: bold;
}

